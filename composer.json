{
    "name": "mostafasy/mezzio-debugbar",
    "type": "library",
    "description": "Middleware to insert PHP DebugBar automatically in html responses",
    "license": "MIT",
    "keywords": [
        "psr-7",
        "psr-15",
        "middleware",
        "server",
        "http",
        "debugbar"
    ],
    "homepage": "https://github.com/mostafasy/mezzio-debugbar",
    "support": {
        "issues": "https://github.com/mostafasy/mezzio-debugbar"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "maximebf/debugbar": "^1.13",
        "laminas/laminas-diactoros": "^2.3",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^1.0 || ^2.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9",
        "laminas/laminas-coding-standard": "~2.5.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "squizlabs/php_codesniffer": "^3.0",
        "phpstan/phpstan": "^0.12",
        "laminas/laminas-config-aggregator": "^1.0",
        "mezzio/mezzio": "^3.0",
        "doctrine/orm": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Mezzio\\DebugBar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mezzio\\DebugBar\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "phpstan": "phpstan analyse",
        "test": "phpunit",
        "coverage": "phpunit --coverage-text",
        "coverage-html": "phpunit --coverage-html=coverage"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
